def get_word(sentence,n):
    if n>0:
        words=sentence .split()
        print(words)
        if n<=len(words):
            return(words[n-1])
    return("")
print(get_word("this is a lesson about list", 4))
print(get_word("this is a lesson about list", -4 ))
print(get_word("now we are cooking",1))
print(get_word("now we are cooking",5))